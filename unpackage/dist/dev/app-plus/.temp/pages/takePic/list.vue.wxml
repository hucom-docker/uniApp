                  <view class="_view data-v-3c99b0c3 list-center-for-bottom-textRight">{{list.time}}</view>
                </view>
                <view class="_view data-v-3c99b0c3">
                  <view class="_view data-v-3c99b0c3 list-center-for-bottom-round"></view>
                  <view class="_view data-v-3c99b0c3 list-center-for-bottom-text">上报人</view>
                  <view class="_view data-v-3c99b0c3 list-center-for-bottom-textRight">{{list.name}}</view>
                </view>
              </view>
              <view class="_view data-v-3c99b0c3 dot-left"></view>
              <view class="_view data-v-3c99b0c3 dot-right"></view>
            </view>
          </scroll-view>
        </view>
      </view>
      <view class="_view data-v-3c99b0c3 list-bottom" bindtap="handleProxy" data-eventid="{{'2'}}" data-comkey="{{$k}}">
        <view class="_view data-v-3c99b0c3 iconfont icon-jia"></view>
        <view class="_view data-v-3c99b0c3 bottomText">新增随手拍</view>
