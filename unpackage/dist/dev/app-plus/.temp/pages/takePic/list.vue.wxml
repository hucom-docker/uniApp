              <view class="_view data-v-3c99b0c3 list-center-for-top">
                <view class="_view data-v-3c99b0c3 iconfont icon-didian"></view>
                <view class="_view data-v-3c99b0c3 list-center-for-text">{{list.address}}</view>
                <view class="_view data-v-3c99b0c3 {{[list.type == '1' ? 'list-center-for-type1' : 'list-center-for-type2']}}">{{list.typeText}}</view>
              </view>
              <view class="_view data-v-3c99b0c3 list-center-for-bottom">
                <view class="_view data-v-3c99b0c3">
                  <view class="_view data-v-3c99b0c3 list-center-for-bottom-round"></view>
