      <view class="_view data-v-3c99b0c3 list-top-warp">
        <view class="_view data-v-3c99b0c3 list-top">
          <view wx:key="index" key="{{index}}" bindtap="handleProxy" data-eventid="{{'0-'+index}}" data-comkey="{{$k}}" wx:for="{{tabArr}}" wx:for-index="index" wx:for-item="item" class="_view data-v-3c99b0c3 {{[item.choose ? 'chooseTab' : '']}}">{{item.text}}</view>
        </view>
      </view>
      <view class="_view data-v-3c99b0c3 list-center-warp">
